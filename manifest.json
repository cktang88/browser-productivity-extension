{
  "manifest_version": 2,

  "name": "ce-incremental",
  "description": "An incremental game played by browsing the web.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },
  "background": {
    "scripts": ["runner.js"],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ]
}